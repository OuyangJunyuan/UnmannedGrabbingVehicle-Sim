<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="Excavator">
  <xacro:include filename="$(find velodyne_description)/urdf/LiDAR-80.xacro"/>
  <xacro:include filename="$(find velodyne_description)/urdf/VLP-16.urdf.xacro"/>
  <xacro:include filename="$(find excavator)/urdf/CameraSensor.xacro"/>



  <xacro:property name="M_PI" value="3.1415926535897931" />
  <xacro:macro name="Macro_transmission" params="name joint actuator">
    <transmission name="${name}">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${joint}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="${actuator}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
  </xacro:macro>
  <xacro:macro name="Macro_RevoluteJoint" params="name  parent child lmt_h:=0 lmt_l:=0 *origin *axis" >
    <joint name="${name}"  type="revolute">
      <parent link="${parent}" />
      <child link="${child}" />
      <xacro:insert_block name="origin" />
      <xacro:insert_block name="axis" />
      <limit  lower="${lmt_l}" upper="${lmt_h}" effort="1e12" velocity="1e12" />
    </joint>
    <xacro:Macro_transmission  name="${name}_tsms" joint="${name}" actuator="${name}_actor"/>
  </xacro:macro>



  <xacro:macro name="Macro_TopLink" params="name:=TopLink mesh:=package://excavator/meshes/Top_link.STL">
    <gazebo reference="${name}">
      <material>Gazebo/Yellow</material>
    </gazebo>
    <link name="${name}">
      <inertial>
        <origin
                xyz="0.85443 0.42482 -0.035362"
                rpy="0 0 0" />
        <mass
                value="244" />
        <inertia
                ixx="43.7"
                ixy="-9.32"
                ixz="-9.81"
                iyy="9"
                iyz="0.499"
                izz="76" />
      </inertial>
      <visual>
        <origin
                xyz="0 0 0"
                rpy="0 0 0" />
        <geometry>
          <mesh filename="${mesh}" />
        </geometry>
        <material  name="">
          <color
                  rgba="1 1 0 1" />
        </material>
      </visual>
      <collision>
        <origin
                xyz="0 0 0"
                rpy="0 0 0" />
        <geometry>
          <mesh  filename="${mesh}" />
        </geometry>
      </collision>
    </link>
  </xacro:macro>
  <xacro:macro name="Macro_MoveBaseLink" params="name:=MoveBaseLink mesh:=package://excavator/meshes/base_link.STL">
    <gazebo reference="${name}">
      <material>Gazebo/Black</material>
    </gazebo>
    <link name="${name}">
      <inertial>
        <origin
                xyz="0.072298 -0.0051598 -4.4577E-09"
                rpy="0 0 0" />
        <mass value="2666.6" />
        <inertia
                ixx="1510.3"
                ixy="-0.52119"
                ixz="3.0279E-06"
                iyy="1455"
                iyz="-4.6553E-07"
                izz="1569.1" />
      </inertial>
      <visual>
        <origin
                xyz="0 0 0"
                rpy="0 0 0" />
        <geometry>
          <mesh
                  filename="${mesh}" />
        </geometry>
        <material
                name="">
          <color
                  rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin
                xyz="0 0 0"
                rpy="0 0 0" />
        <geometry>
          <mesh
                  filename="${mesh}" />
        </geometry>
      </collision>
    </link>
  </xacro:macro>
  <xacro:macro name="Macro_ArmLink1" params="name:=ArmLink1 mesh:=package://excavator/meshes/arm_link1.STL">
    <gazebo reference="${name}">
