  <xacro:macro name="Macro_ArmLink3" params="name:=ArmLink3 mesh:=package://excavator/meshes/arm_link3.STL">
    <gazebo reference="${name}">
      <material>Gazebo/Orange</material>
    </gazebo>
    <link name="${name}">
