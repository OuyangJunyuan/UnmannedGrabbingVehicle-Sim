                rpy="0 0 0" />
        <mass
                value="224.3" />
        <inertia
                ixx="147.1"
                ixy="50.61"
                ixz="6.3581E-13"
                iyy="33.65"
                iyz="-1.2935E-12"
                izz="178" />
      </inertial>
      <visual>
        <origin
                xyz="0 0 0"
                rpy="0 0 0" />
